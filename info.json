{
    "manufacturer": "SiegeCMD",
    "keyboard_name": "cmdpad_",
    "maintainer": "SiegeCMD",
    "bootloader": "caterina",
    "diode_direction": "COL2ROW",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": false,
        "extrakey": true,
        "mousekey": true,
        "nkro": true
    },
    "matrix_pins": {
        // see https://golem.hu/pic/pro_micro_pinout.jpg
            "direct" : [
            ["D0", "C6", "B3", "F5"],
            ["D4", "D7", "B2", "B1"]      
            ]
    },
    "processor": "atmega32u4",
    "url": "",
    "usb": {
        "device_version": "1.0.0",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "layouts": {
        "LAYOUT_ortho_4x2": {
            "layout": [
                // no need for rows because I wired it direct
                {"x": 0, "y": 0, "matrix": [0, 0]},
                {"x": 1, "y": 0, "matrix": [0, 1]},
                {"x": 2, "y": 0, "matrix": [0, 2]},
                {"x": 3, "y": 0, "matrix": [0, 3]}, // encoder
                {"x": 0, "y": 1, "matrix": [1, 0]},
                {"x": 1, "y": 1, "matrix": [1, 1]},
                {"x": 2, "y": 1, "matrix": [1, 2]},
                {"x": 3, "y": 1, "matrix": [1, 3]}
            ]
        }
    }
}